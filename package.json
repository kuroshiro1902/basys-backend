{
  "name": "basys-server",
  "version": "0.0.1",
  "description": "basys server.",
  "author": "Jason",
  "repository": "kuroshiro1902/basys-server",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "start:dev": "npx prisma migrate dev && npx prisma db seed && tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "npx prisma migrate deploy && npx prisma generate && npx prisma db seed && tsup",
    "start": "node build/index.js",
    "test": "tsx watch --clear-screen=false src/test.ts | pino-pretty"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "pg": "^8.13.1",
    "uid": "^2.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node-schedule": "^2.1.7",
    "pino-pretty": "^11.0.0",
    "prisma": "^6.4.1",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "tsup": "^8.0.2"
  }
}
